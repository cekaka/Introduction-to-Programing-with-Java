/*
 * Write a method, which print integer number in reverse order.
 * Example: 432--->234
 */
import java.util.*;
public class CH9_07 {

	public static void printNumberRevOrder(int number){
		do{
			System.out.print(number%10);
			number=number/10;
		}while(number>0);	
	}
	
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		System.out.println("Enter a number");
		int num = input.nextInt();
		
		printNumberRevOrder(num);

	}

}
