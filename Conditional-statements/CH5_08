
/*
 * Write a program, that reads a ciphers - (1..999) and prints the name of them on bulgarian.
 */
import java.util.*;
public class CH5_08 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		System.out.println("Enter a number between[99..999]");
		int num = input.nextInt();
		
		
		if(num%100==0){
			int numHun = num/100;
			
			switch(numHun){
				case 1:System.out.print("Сто ");break;
				case 2:System.out.print("Двеста ");break;
				case 3:System.out.print("Триста ");break;
				case 4:System.out.print("Четристотин ");break;
				case 5:System.out.print("Петстотин ");break;
				case 6:System.out.print("Шестотин ");break;
				case 7:System.out.print("Седемстотин ");break;
				case 8:System.out.print("Осемстотин ");break;
				case 9:System.out.print("Деветстотин ");break;

				default:System.out.println("Числото е извън интервала!!! ");break;
		    }
		}
		
		if(num%100>=1){
			int numHun = num/100;
			int numTen = (num%100)/10;
			int numTen2 = num%100;
			int numUnt = num%10;
			
			
				if(numTen>1){
					
					switch(numHun){
						case 0:System.out.print("");break;
						case 1:System.out.print("Сто ");break;
						case 2:System.out.print("Двеста ");break;
						case 3:System.out.print("Триста ");break;
						case 4:System.out.print("Четристотин ");break;
						case 5:System.out.print("Петстотин ");break;
						case 6:System.out.print("Шестотин ");break;
						case 7:System.out.print("Седемстотин ");break;
						case 8:System.out.print("Осемстотин ");break;
						case 9:System.out.print("Деветстотин ");break;

						default:System.out.println("Числото е извън интервала! ");break;
					}
						
					switch(numTen){
					  case 0:break;
						case 2:System.out.print("Двaдесет и ");break;
						case 3:System.out.print("Тридесет и ");break;
						case 4:System.out.print("Четиредесет и ");break;
						case 5:System.out.print("Петдесет и ");break;
						case 6:System.out.print("Шестдесет и ");break;
						case 7:System.out.print("Седемдесет и ");break;
						case 8:System.out.print("Осемдесет и ");break;
						case 9:System.out.print("Деведесет и ");break;

						default:System.out.println("Грешкааа!!! ");break;
				
					}
					switch(numUnt){
				   
						case 1: System.out.print("едно");break;
						case 2: System.out.print("две");break;
						case 3: System.out.print("три");break;
						case 4: System.out.print("четири");break;
						case 5: System.out.print("пет");break;
						case 6: System.out.print("шест");break;
						case 7: System.out.print("седем");break;
						case 8: System.out.print("осем");break;
						case 9: System.out.print("девет");break;
						default: System.out.print("Не знам ко е това число");
					}
				}
					
					else{
						switch(numHun){
							case 0:System.out.print("");break;
							case 1:System.out.print("Сто и ");break;
							case 2:System.out.print("Двеста и ");break;
							case 3:System.out.print("Триста и ");break;
							case 4:System.out.print("Четристотин и ");break;
							case 5:System.out.print("Петстотин и ");break;
							case 6:System.out.print("Шестотин и ");break;
							case 7:System.out.print("Седемстотин и");break;
							case 8:System.out.print("Осемстотин и ");break;
							case 9:System.out.print("Деветстотин и ");break;

							default:System.out.println("Числото е извън интервала!!! ");break;
						}
						switch(numTen2){
								case 1: System.out.print("едно");break;
								case 2: System.out.print("две");break;
								case 3: System.out.print("три");break;
								case 4: System.out.print("четири");break;
								case 5: System.out.print("пет");break;
								case 6: System.out.print("шест");break;
								case 7: System.out.print("седем");break;
								case 8: System.out.print("осем");break;
								case 9: System.out.print("девет");break;
								case 10:System.out.println("Десет ");break;
								case 11:System.out.println("Идинадесет ");break;
								case 12:System.out.println("Дванадесет ");break;
								case 13:System.out.println("Тринадесет ");break;
								case 14:System.out.println("Четиринадесет ");break;
								case 15:System.out.println("Петнадесет");break;
								case 16:System.out.println("Шестнадесет ");break;
								case 17:System.out.println("Седемнадесет ");break;
								case 18:System.out.println("Осемнадесет ");break;
								case 19:System.out.println("Деветнадесет ");break;

								default:System.out.println("Грешка!!! ");break;
							
						}
					}
					
					
			}

	}

}
