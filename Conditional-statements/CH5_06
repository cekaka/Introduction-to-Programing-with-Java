/*
* Write a program, which finds the biggest number between five given numbers.
*/
 

import java.util.*;
public class CH5_06 {


	public static void main(String[] args) {
		
	Scanner input = new Scanner(System.in);
	System.out.println("Enter  a number:");
	double firNum = input.nextDouble();
	
	double bufNum = firNum;
	
	
	for(int i=0;i<4;i++){
		
		System.out.println("Enter  a number:");
		double num = input.nextDouble();
		
		if(bufNum<num){
			bufNum=num;
		}
	}
	
	System.out.println("The biggest number is :"+bufNum);
	
	}

}
