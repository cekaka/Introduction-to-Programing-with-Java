/*
 * Write a program, that makes a Array whith 20 integer elements,
 * the values of all of them is his index miltiplied by 5.
 * Print the elements.
 */
import java.util.*;
public class CH7_01 {


	public static void main(String[] args) {
		int[] Array = new int[20];
		
		for(int index=0;index<Array.length;index++){
			Array[index]=index*5;
		}
		
		for(int i=0;i<Array.length;i++){
			System.out.println(Array[i]);
		}
					
	}

}
